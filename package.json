{
  "name": "starter-next",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier -w .",
    "eslint": "eslint",
    "format:check": "prettier -c .",
    "icons": "npx @svgr/cli --title-prop --icon --typescript -d ./src/icons/components ./src/icons/svg && npx prettier --write ./src/icons/components",
    "cy:run": "cypress run --browser chrome --headless",
    "cmp": "npx hygen cmp new",
    "sub": "npx hygen cmp sub",
    "prepare": "husky install",
    "exp": "node ./expansion.js"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwind-merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.0",
    "@storybook/manager-webpack5": "^6.5.0",
    "@storybook/react": "^6.5.0",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "change-case": "^4.1.2",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "{src,pages}/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
